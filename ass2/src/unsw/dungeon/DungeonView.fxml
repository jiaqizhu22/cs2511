<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>

<BorderPane fx:id="border" onKeyPressed="#handleKeyPress" xmlns="http://javafx.com/javafx/11.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane fx:id="squares" alignment="CENTER" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <ButtonBar prefHeight="40.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="replayBtn" mnemonicParsing="false" text="Replay" />
            <Button fx:id="menuBtn" layoutX="125.0" layoutY="17.0" mnemonicParsing="false" text="Menu" />
        </buttons>
      </ButtonBar>
   </top>
</BorderPane>
